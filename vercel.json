{
    "version": 2,
    "name": "ediaru",
    "builds": [
       {
         "src": "manage.py",
         "use": "@vercel/python",
         "config": {
           "build": {
             "env": {
               "PIP_DISABLE_PIP_VERSION_CHECK": "1"
             }
           }
         }
       }
    ],
    "routes": [
       {
         "src": "/static/(.*)",
         "dest": "/static/$1"
       },
       {
         "src": "/media/(.*)",
         "dest": "/media/$1"
       },
       {
         "src": "/(.*)",
         "dest": "manage.py"
       }
    ]
   }